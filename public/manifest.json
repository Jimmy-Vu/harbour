{
  "manifest_version": 3,
  "name": "Harbour",
  "version": "1.0",
  "description": "Tagging the internet, one bookmark at a time.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png"
  }
}
